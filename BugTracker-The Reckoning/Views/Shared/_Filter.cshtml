@model BugTracker_The_Reckoning.Models.FilterModel

<h3>Filter by:</h3>
<div class="row">
</div>
@using (Html.BeginForm("Index", "Tickets", FormMethod.Post)) { 
    @Html.AntiForgeryToken()
    <div class="form-group">
        @Html.LabelFor(model => model.assignedList, "Assigned Developer", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.ListBoxFor(model => model.assignedList, Model.assignedList, htmlAttributes: new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.assignedList, "", new { @class = "text-danger" })
        </div>
    </div>

}
